 import java.util.*;

class Node {
    int key;
    Node  left , right;

    public Node(int item) {
        key = item;
        left = right = null;
    }
}
class BT{

    public static void main(String[] args) {
        Node root;
        root = new Node(1);
        root.left = new Node(2);
        root.right = new Node(3);
        System.out.println(root.right.key);
    }

}
